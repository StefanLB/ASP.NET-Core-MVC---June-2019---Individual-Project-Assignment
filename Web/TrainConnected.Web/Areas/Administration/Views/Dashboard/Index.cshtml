@{
    this.ViewData["Title"] = "Admin dashboard";
}

<div class="container-fluid text-md-center mb-5">
    <h1>@this.ViewData["Title"]</h1>
</div>


<div class="w-100">
        <div class="text-center w-50 mb-3 border-bottom">
            <h3 asp-area="Administration" asp-controller="WorkoutActivities" asp-action="All">Workout Activities</h3>
        </div>
        <div class="text-center w-50 mb-3 border-bottom">
            <h3 asp-area="Administration" asp-controller="PaymentMethods" asp-action="All">Payment Methods</h3>
        </div>
        <div class="text-center w-50 mb-3 border-bottom">
            <h3 asp-area="Administration" asp-controller="Withdrawals" asp-action="All">Withdrawals</h3>
        </div>
        <div class="text-center w-50 mb-3 border-bottom">
            <h3 asp-area="Administration" asp-controller="Users" asp-action="All">Users</h3>
